<!doctype html>
<html>
<head>
    <title>COVID-19 Symptoms</title>
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css"> 

    <link rel="stylesheet" href="style.css">
</head>
<body>

<div class="contain">

  <div class="page-header text-center">
    <h1 class="header">COVID-19 Symptoms</h1>
    <nav>
        <a href="/profile" class="btn btn-default btn-sm">Profile</a>
        <a href="/logout" class="btn btn-default btn-sm">Logout</a>
		<a href="/deleted" class="btn btn-default btn-sm">Archived</a>
    </nav>
  </div>
        <div class="col-sm-6">

          <ul class="messages">
          <% for(var i=0; i<messages.length; i++) { %>
            <% if(messages[i].delete == 'yes'){ %>
              <% if( messages[i].name == user.local.email){ %>
            <li class="message">
              <span id="emailHold"><%= messages[i].name %></span>
              <span><%= messages[i].msg %></span>
              <span><%= messages[i].date %></span>
            </li>
          <% } %>
            <% } %>
            <% } %>
          </ul>
	</div>


<script src="main.js"></script>

</body>
</html>